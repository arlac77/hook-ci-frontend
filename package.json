{
  "name": "hook-ci-frontend",
  "description": "hook-ci web frontend",
  "version": "0.0.0-semantic-release",
  "keywords": [
    "ci",
    "web"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "scripts": {
    "start": "node ./browser-sync.js",
    "startx": "rollup -c rollup.config.js -w",
    "prepare": "rollup -c rollup.config.js",
    "package": "npm-pkgbuild pkgbuild pacman makepkg"
  },
  "devDependencies": {
    "browser-sync": "^2.26.5",
    "express": "^4.16.4",
    "http-proxy-middleware": "^0.19.1",
    "npm-pkgbuild": "^5.2.9",
    "rollup": "^1.11.2",
    "rollup-plugin-copy": "^2.0.1",
    "semantic-release": "^15.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/hook-ci-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/hook-ci-frontend/issues"
  },
  "homepage": "https://github.com/arlac77/hook-ci-frontend#readme",
  "pacman": {
    "groups": "ci",
    "content": {
      "/services/hook-ci/docroot/index.html": "build/index.html",
      "/services/hook-ci/docroot/bundle.js": "build/bundle.js"
    },
    "depends": {
      "hook-ci": ""
    }
  }
}
