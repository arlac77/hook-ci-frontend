{
  "name": "hook-ci-frontend",
  "description": "hook-ci web frontend",
  "version": "0.0.0-semantic-release",
  "keywords": [
    "ci",
    "web"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "scripts": {
    "start": "rollup -c rollup.config.js -w",
    "test": "ava",
    "prepare": "rollup -c rollup.config.js",
    "package": "npm-pkgbuild pkgbuild pacman makepkg"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "@semantic-release/exec": "^3.3.3",
    "browser-sync": "^2.26.5",
    "express": "^4.16.4",
    "http-proxy-middleware": "^0.19.1",
    "npm-pkgbuild": "^6.0.5",
    "puppeteer": "^1.15.0",
    "rollup": "^1.11.3",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-copy": "^2.0.1",
    "rollup-plugin-css-only": "^1.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-vue": "^5.0.0",
    "semantic-release": "^15.13.12",
    "vue-template-compiler": "^2.6.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/hook-ci-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/hook-ci-frontend/issues"
  },
  "homepage": "https://github.com/arlac77/hook-ci-frontend#readme",
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "npx npm-pkgbuild --pkgver ${nextRelease.version} pkgbuild pacman makepkg"
        }
      ]
    ]
  },
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "pacman": {
    "groups": "ci",
    "installdir": "/services/hook-ci/frontend",
    "content": {
      "${installdir}/": {
        "base": "build/dist",
        "pattern": "**/*"
      }
    },
    "depends": {
      "hook-ci": ">=1.2.0"
    }
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.19",
    "vue": "^2.6.10",
    "vue-filter-pretty-bytes": "^0.1.2",
    "vue-moment-lib": "^1.2.2",
    "vue-router": "^3.0.6"
  }
}